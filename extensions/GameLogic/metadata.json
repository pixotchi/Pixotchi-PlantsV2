    "devdoc": {
      "details": "Implements the core game logic for the Pixotchi game.",
      "kind": "dev",
      "methods": {
        "attack(uint256,uint256)": {
          "details": "Allows a plant to attack another plant.",
          "params": {
            "fromId": "The ID of the attacking plant.",
            "toId": "The ID of the plant being attacked."
          }
        },
        "getAllStrainInfo()": {
          "details": "Gets information about all active strains.",
          "returns": {
            "_0": "IGame.Strain[] An array of active strains."
          }
        },
        "getPlantName(uint256)": {
          "details": "Gets the name of a plant.",
          "params": {
            "_id": "The ID of the plant."
          },
          "returns": {
            "_0": "string The name of the plant."
          }
        },
        "getStatus(uint256)": {
          "details": "Gets the status of a plant.",
          "params": {
            "plant": "The ID of the plant."
          },
          "returns": {
            "_0": "Status The status of the plant."
          }
        },
        "isApprovedFn(uint256,address)": {
          "details": "Checks if the given wallet is approved for the specified plant ID.",
          "params": {
            "id": "The ID of the plant.",
            "wallet": "The address of the wallet."
          },
          "returns": {
            "_0": "bool True if the wallet is approved, false otherwise."
          }
        },
        "isPlantAlive(uint256)": {
          "details": "Checks if a plant is alive.",
          "params": {
            "_nftId": "The ID of the plant."
          },
          "returns": {
            "_0": "bool True if the plant is alive, false otherwise."
          }
        },
        "kill(uint256,uint256)": {
          "details": "Allows a player to kill a dead plant and claim its points.",
          "params": {
            "_deadId": "The ID of the dead plant.",
            "_tokenId": "The ID of the player's plant."
          }
        },
        "level(uint256)": {
          "details": "Gets the level of a plant.",
          "params": {
            "tokenId": "The ID of the plant."
          },
          "returns": {
            "_0": "uint256 The level of the plant."
          }
        },
        "onAttack(uint256,uint256)": {
          "details": "Handles an attack between two plants.",
          "params": {
            "fromId": "The ID of the attacking plant.",
            "toId": "The ID of the plant being attacked."
          },
          "returns": {
            "canAttack": "True if the attack can proceed, false otherwise.",
            "odds": "The odds of winning the attack.",
            "pct": "The percentage of the prize."
          }
        },
        "pass(uint256,uint256)": {
          "details": "Allows a player to pass the diamond to another plant.",
          "params": {
            "from": "The ID of the plant passing the diamond.",
            "to": "The ID of the plant receiving the diamond."
          }
        },
        "pendingEth(uint256)": {
          "details": "Gets the pending ETH rewards for a plant.",
          "params": {
            "plantId": "The ID of the plant."
          },
          "returns": {
            "_0": "uint256 The pending ETH rewards."
          }
        },
        "redeem(uint256)": {
          "details": "Redeems the rewards for a plant.",
          "params": {
            "id": "The ID of the plant."
          }
        },
        "setPlantName(uint256,string)": {
          "details": "Sets the name of a plant.",
          "params": {
            "_id": "The ID of the plant.",
            "_name": "The new name of the plant."
          }
        },
        "statusToString(uint8)": {
          "details": "Converts a Status enum to a string.",
          "params": {
            "status": "The Status enum."
          },
          "returns": {
            "_0": "string The string representation of the status."
          }
        }
      },
      "title": "GameLogic",
      "version": 1
